{
    "name": "nisimpo/blog",
    "description": "A package to add website in any laravel application",
    "type": "library",
    "authors": [
        {
            "name": "nzogo",
            "email": "boss.sm11@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "illuminate/console": "^9.21|^10.0|^11.0",
        "illuminate/filesystem": "^9.21|^10.0|^11.0",
        "illuminate/support": "^9.21|^10.0|^11.0",
        "illuminate/validation": "^9.21|^10.0|^11.0",
        "symfony/console": "^6.0|^7.0",
        "livewire/livewire": "^3.0",
        "qcod/laravel-settings": "dev-master",
        "realrashid/sweet-alert": "dev-master"
    },
    "require-dev": {
        "orchestra/testbench": "^7.35|^8.15|^9.0",
        "phpunit/phpunit": "^9.3|^10.4|^11.0"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Nisimpo\\Blog\\": "src/",
            "Illuminate\\Nisimpo\\Blog\\": "app/"
        },
        "files": ["src/Helpers/blog_helpers.php"]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Nisimpo\\Blog\\BlogServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev"
}
